<expression>

unaryExpression
  : '+' _
  | '-' _
  ;

prefixExpression
  : _ '++'
  | _ '--'
  ;

multiplicativeExpression
  : _ '*' _
  | _ '/' _
  | _ '%' _
  ;

additiveExpression
  : _ '+' _
  | _ '-' _
  ;

relationalExpression
  : _ '<' _
  | _ '>' _
  | _ '<=' _
  | _ '>=' _
  ;

conditionalExpression
  : _ '?' _ ':' _
  ;